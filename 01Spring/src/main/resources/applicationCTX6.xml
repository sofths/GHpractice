<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:c="http://www.springframework.org/schema/c"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="student3" class="com.human.ex8.Student">
		<constructor-arg value="홍길자"></constructor-arg>
		<constructor-arg value="8"></constructor-arg>
		<constructor-arg>
			<list>
				<value>줄넘기</value>
				<value>공기놀이</value>
			</list>
		</constructor-arg>
		<property name="height">
			<value>157</value>
		</property>
		<property name="weight" value="34"></property>
	</bean>
	
	<bean id="familly" class="com.human.ex8.Familly" 
	c:papaName="홍아빠" c:mamiName="홍엄마" p:sisterName="홍누나">
		<property name="brotherName" value="홍오빠"/>
	</bean>


</beans>
