<mapper namespace="mapper.user">

	<insert id="insertUser"  parameterType="user">
		<![CDATA[
			 insert into tbl_users(username, password, enabled)
			 values(#{username}, #{password}, #{enabled})
		]]>      
   </insert>
  
</mapper>